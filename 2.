a=3
b=3
k = [[0,0,0],[0,0,0],[0,0,0]]
def main(k):
    for r in range(a):
        for c in range(b):
            k[r][c] = int(input())
    if k[0][0] + k[0][1] + k[0][2] == k[1][0] + k[1][1] + k[1][2] == k[1][0] + k[1][1] + k[1][2] == k[2][0] + k[2][1] + k[2][2] == k[0][2] + k[1][2] + k[2][2] == k[0][0] + k[1][1] + k[2][2]:
        print('Это магический квадрат Ло Шу')
    else:
        print ('Это не магический квадрат Ло Шу')
    return(k)
print('Проверка квадрата Ло Шу\nКаждая тройка цифр является рядом квадрата\nНачинайте вводить числа от 1 до 9 через "enter": ')
main(k)
print(k)
